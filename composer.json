{
    "name": "guave/contao-visualradio",
    "description": "",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Rodrigo Stadler",
            "email": "rodrigo.stadler@guavestudios.com",
            "homepage": "https://github.com/guavestudios"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "contao/core-bundle": ">=4.4 || <5.0",
        "symfony/framework-bundle": "^3.4 || ^4.0"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-4.6": "4.6.x-dev"
        },
        "contao-manager-plugin": "Guave\\Visualradio\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Guave\\Visualradio\\": "src/"
        },
        "classmap": ["src/Resources/contao/"],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
